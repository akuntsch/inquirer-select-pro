{
  "name": "inquirer-select-pro",
  "version": "0.1.0",
  "description": "An inquirer select that supports multiple selections and filtering.",
  "keywords": [
    "inquirer",
    "prompts",
    "cli",
    "select",
    "multi"
  ],
  "license": "MIT",
  "author": "jeffwcx <jeffwcx@icloud.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./index/index.js",
      "types": "./index.d.ts"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsx ./examples/cli.ts",
    "format": "eslint . --cache --fix",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 --cache",
    "prepare": "husky",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,mjs,cjs,ts,tsx,mts,cts,json}": "pnpm format",
    "**/*.{md}": "prettier --write --ignore-unknown"
  },
  "prettier": "@jeffwcx/prettier-config",
  "dependencies": {
    "@inquirer/core": "^8.1.0",
    "@inquirer/figures": "^1.0.1",
    "@inquirer/type": "^1.3.1",
    "@types/lodash-es": "^4.17.12",
    "ansi-escapes": "^7.0.0",
    "chalk": "^5.3.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@inquirer/testing": "^2.1.18",
    "@jeffwcx/eslint-config": "^0.3.0",
    "@jeffwcx/prettier-config": "^0.1.2",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^20.12.10",
    "@vitest/coverage-v8": "^1.6.0",
    "cli-spinners": "^3.0.0",
    "eslint": "^9.2.0",
    "flexsearch": "^0.7.43",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5",
    "vitest": "1.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
